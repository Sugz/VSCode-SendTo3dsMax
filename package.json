{
  "name": "send-to-3dsmax",
  "displayName": "Send to 3dsMax",
  "description": "Execute Maxscript and Python in 3dsMax",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:send-to-3dsmax.send",
    "onCommand:send-to-3dsmax.select"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "send-to-3dsmax.send",
        "title": "Send to 3dsMax"
      },
      {
        "command": "send-to-3dsmax.select",
        "title": "Select 3dsMax instance"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.26.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "ffi-napi": "^4.0.3"
  }
}
